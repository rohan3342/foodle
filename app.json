{
  "expo": {
    "name": "foodle",
    "slug": "foodle",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/foodle-icon@2x.png",
    "splash": {
      "image": "./assets/splash/foodle-splash@1x.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/foodle-icon@2x.png",
        "backgroundColor": "#FFFFFF"
      }
    }
  }
}